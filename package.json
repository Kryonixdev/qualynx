{
  "name": "qualynx",
  "version": "1.0.0",
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kryonixdev/qualynx.git"
  },
  "description": "Generic Playwright Framework for API, Mobile and UI Testing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "ts-node scripts/build.ts",
    "clean": "tsc --build --clean",
    "prepublishOnly": "npm install && npm run build && npm run copy-assets",
    "copy-assets": "mkdir -p dist/assets && cp -r src/assets/* dist/assets/",
    "test": "npx playwright test --config=./src/playwright.config.ts",
    "docs": "typedoc",
    "report": "npx playwright show-report",
    "update": "npx npm-check-updates -u && npm install && npm run build"
  },
  "keywords": [
    "playwright",
    "browserstack",
    "xray",
    "automation",
    "api testing",
    "typescript",
    "testing",
    "mobile testing",
    "graphql"
  ],
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.996.0",
    "@aws-sdk/client-rds-data": "3.996.0",
    "@aws-sdk/client-s3": "3.996.0",
    "@aws-sdk/client-secrets-manager": "3.996.0",
    "@aws-sdk/client-sqs": "3.996.0",
    "@aws-sdk/client-ssm": "3.996.0",
    "@aws-sdk/s3-request-presigner": "3.996.0",
    "@aws-sdk/types": "3.973.1",
    "@aws-sdk/util-dynamodb": "3.996.0",
    "@aws-sdk/util-endpoints": "3.996.0",
    "@axe-core/playwright": "4.11.1",
    "@estruyf/github-actions-reporter": "1.11.0",
    "@playwright/test": "1.58.2",
    "@types/node": "25.3.0",
    "@types/pixelmatch": "5.2.6",
    "@types/pngjs": "6.0.5",
    "@xray-app/playwright-junit-reporter": "0.10.0",
    "axios": "1.13.5",
    "browserstack-node-sdk": "1.49.11",
    "dotenv": "17.3.1",
    "fs-extra": "11.3.3",
    "monocart-reporter": "2.10.0",
    "npm-check-updates": "19.4.1",
    "pixelmatch": "7.1.0",
    "playwright": "1.58.2",
    "pngjs": "7.0.0",
    "slack-notify": "2.0.7",
    "ts-node": "10.9.2",
    "typedoc": "0.28.17",
    "typedoc-plugin-markdown": "4.10.0",
    "typescript": "5.9.3",
    "v8-to-istanbul": "9.3.0",
    "winston": "3.19.0"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.4"
  },
  "overrides": {
    "chalk": "4.1.2",
    "undici": "^6.23.0"
  }
}
